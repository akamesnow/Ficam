prapid$    # Ligne rapide
	if ss$ <> old_ss, n$, ssa, *spdlon, e$
	n$, sgcode, x$, y$, z$,e$
	old_ss = ss$
pzrapid$   # Ligne Z rapide
	if ss$ <> old_ss, n$, ssa, *spdlon, e$
	n$, sgcode, z$,e$
	old_ss = ss$
plin$      # Ligne travail
	if ss$ <> old_ss, n$, ssa, *spdlon, e$
	n$, sgcode, pcord, x$, y$, z$, pfr,e$
	old_ss = ss$
pz$        # z en avance rapide
	if ss$ <> old_ss, n$, ssa, *spdlon, e$
	n$, sgcode, z$, pfr,e$
	old_ss = ss$
pcir$      # Circulaire
	if plane$ = 0, n$, splcode, *sgcode, x$, y$, z$, i$, j$, pfr,e$
	if plane$ = 2, n$, splcode, *sgcode, x$, z$, y$, i$, k$, pfr,e$
	if plane$ = 1, n$, splcode, *sgcode, y$, z$, x$, j$, k$, pfr,e$
pdwl_spd$
	if ss$ <> old_ss, 
	[
		n$, ssa, *spdlon, e$
		old_ss = ss$
	]
	if dwell$ <> 0,
	[
		tempo = dwell$
		*sgcode , *tempo , e$
	]
#-------------------------------------------------------------------------
phole       # trous suivants
	n$, x$, y$, depth$, refht$, e$

pdrill$    # Cycle de percage G81 G82
	#n$, "(POINTAGE-PERCAGE)",e$
	if dwell$ = 0, n$, "G81", *x$, *y$, *depth$, *refht$, pfrForce, e$
	else, n$, "G82", *x$, *y$, *depth$, *refht$, pfrForce, [if dwell$ <> 0, *dwell$], e$
ppeck$     # Cycle de debourrage G83
	#n$, "(DEBOURRAGE)",e$
	n$, "G83", *x$, *y$, *depth$, *refht$, peck1$, pfrForce, [if dwell$ <> 0, *dwell$],e$
pchpbrk$   # Cycle de debourrage G83
	#n$, "(BRISE COPEAUX)",e$
	n$, "G73", *x$, *y$, *depth$, *refht$, peck1$, pfrForce, [if dwell$ <> 0, *dwell$],e$
