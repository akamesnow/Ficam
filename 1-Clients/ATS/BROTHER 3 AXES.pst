	]
pCreerBuffer
	cont = 0
	sbufname2$ = ssynced_buffer2
	buffer_2_size = rbuf(2, 0)
	rc2 = 1
	find = no$
	while rc2 <= buffer_2_size,
	[
		spaces$ = 0
		valA = rbuf(2,rc2)
		if valB = workofs$,
		[
			find = yes$
			rc2 = buffer_2_size + 1
		]
		spaces$ = 1
	]
	if find = no$,
	[
		valA = incOri
		valB = workofs$
		valA = wbuf (2, wc2)
	]
