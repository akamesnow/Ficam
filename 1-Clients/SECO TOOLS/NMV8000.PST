            serreur2 = serreur + no2str(numop)
            result = mprint(serreur2, 3)

            if result = 3, serreur2 , e$, exitpost$ # abandonner
            if result = 4, serreur2         # recommencer
            if result = 5,  testerreur4 = 1     # ignorer

