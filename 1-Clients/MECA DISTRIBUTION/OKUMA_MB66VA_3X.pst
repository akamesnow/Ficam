				]
			]
			else,   
			[
				angle_autour_X2 = 0
				angle_autour_Z2 = atan2(-m2$,m1$)
				angle_autour_Zp = 0
			]
		]
		if combinaison_axes = 2 | combinaison_axes = 3 ,    
