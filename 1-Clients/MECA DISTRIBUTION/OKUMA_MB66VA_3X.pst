    	X_tourne = ydep  * sin(tourne) + xdep  * cos(tourne)
    	Y_tourne = ydep  * cos(tourne) - xdep * sin(tourne)
		I_tourne = j$  * sin(tourne) + i$  * cos(tourne)
    	J_tourne = j$  * cos(tourne) - i$ * sin(tourne)
	]
	else ,
	[
		X_tourne = xdep
		Y_tourne = ydep
		J_tourne = j$
