p_filtre
    !tx
    !ty
    !tz
    !old_tx
    !old_ty
    !old_ty
    #"voir", *old_tx, *tx, *old_ty, *ty, e$
    if axeab_reel <> 0,    
	[
		if tx <> 0 | ty <> 0,    
		[
			if (old_tx <= 0 & tx > 0 & old_ty = 0 & ty = 0)
			| (old_tx >= 0 & tx < 0 & old_ty = 0 & ty = 0)
			| (old_tx = 0 & tx = 0 & old_ty >= 0 & ty < 0)
			| (old_tx = 0 & tx = 0 & old_ty <= 0 & ty > 0)
			| (old_tx < 0 & tx > 0 & old_ty < 0 & ty > 0)
			| (old_tx > 0 & tx < 0 & old_ty > 0 & ty < 0)
			| (old_tx = 0 & tx > 0 & old_ty = 0 & ty < 0)
			| (old_tx = 0 & tx < 0 & old_ty = 0 & ty > 0)
