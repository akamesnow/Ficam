EXTERN RNC_9764(REAL,REAL,REAL,REAL,REAL,REAL,REAL)
EXTERN RNC_9766(REAL,REAL,REAL,REAL,REAL)
;REN OFF CENTRE MEASURE
DEF REAL _REN[33]
