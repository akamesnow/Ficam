PROC RNC_9868(REAL _BB,REAL _DD,REAL _GG,REAL _HH,REAL _II,REAL _JJ,REAL _LL,REAL _MM,REAL _QQ,REAL _RR,REAL _SS,REAL _YY,REAL _ZZ)SAVE
EXTERN RNC_9762(REAL,REAL,REAL,REAL,REAL)
