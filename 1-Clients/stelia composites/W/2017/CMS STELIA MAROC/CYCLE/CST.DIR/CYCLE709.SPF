;classic COUPDEL(FS,LS)
PROC CYCLE709(AXIS _CPF=NO_AXIS,AXIS _CPL=NO_AXIS) IPTRLOCK SBLOF DISPLOF
IF ($P_TECCYCLE==TRUE) GOTOF _FEHL
IF ($PA_CPSETTYPE[_CPF]<>"COUP") GOTOF _EXIT
IF $P_SUBPAR[2]==TRUE
CPDEL=(_CPF) CPLA=(_CPL) CPSETTYPE="COUP" CPFMOF="CONT"
ELSE
CPDEL=(_CPF) CPSETTYPE="COUP" CPFMOF="CONT"
ENDIF
RET
_FEHL: SETAL(61017)
_EXIT: RET
