;classic LEADOF(FA,LA), VL-Stop
PROC CYCLE703(AXIS _CPF=NO_AXIS, AXIS _CPL=NO_AXIS) IPTRLOCK SBLOF DISPLOF ICYCOF
IF ($P_TECCYCLE==TRUE)
IF ($AA_CPSETTYPE[_CPF]<>"LEAD") GOTOF _EXIT
ELSE
IF ($PA_CPSETTYPE[_CPF]<>"LEAD") GOTOF _EXIT
ENDIF
IF _CPL==NO_AXIS
CPSETTYPE[_CPF]="LEAD" CPOF=(_CPF)
ELSE
CPSETTYPE[_CPF]="LEAD" CPLOF[_CPF]=(_CPL)
ENDIF
_EXIT: RET
