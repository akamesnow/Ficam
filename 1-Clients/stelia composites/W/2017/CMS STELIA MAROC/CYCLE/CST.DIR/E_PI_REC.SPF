_REF=_E_TR[_IDD,13] _X0A=_E_TR[_IDD,15]
_Y0A=_E_TR[_IDD,17] _R =_E_TR[_IDD,20]
_A0 =_E_TR[_IDD,21] _DXY=_E_TR[_IDD,22] _UXY=_E_TR[_IDD,23] _L1 =_E_TR[_IDD,24]
_W1 =_E_TR[_IDD,25] _FS =_E_TR[_IDD,26]
