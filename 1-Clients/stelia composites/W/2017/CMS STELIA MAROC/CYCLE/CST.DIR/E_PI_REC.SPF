_AMODE3=100
ENDIF
IF(_E_POS_RP==0)AND(_OB<>4)
_RTP=_ZREF-(_SC+_SD)*_PMZ
ELSE
_RTP=_RP-_SD*_PMZ
ENDIF
IF(_OB==4)
N38 CYCLE76(_RTP,_Z0,_SC,_Z1,,_L,_W,_R,_X0,_Y0,_A0,_DZ,_UXY,_UZ,_F,_F,_E_DIR,_BA1,_L1,_W1,_FS,_ZFS,_GMODE+200,0,2+_AMODE3)
