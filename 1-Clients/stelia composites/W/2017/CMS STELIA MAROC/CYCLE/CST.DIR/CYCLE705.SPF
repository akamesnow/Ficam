;classic COUPON(FS,LS,POSFS)
PROC CYCLE705(AXIS _CPF=NO_AXIS, AXIS _CPL=NO_AXIS, REAL _CPFP) IPTRLOCK SBLOF DISPLOF
IF ($P_TECCYCLE==TRUE) GOTOF _FEHL
IF $P_SUBPAR[2]==TRUE
IF $P_SUBPAR[3]==TRUE
CPON=(_CPF) CPLA=(_CPL) CPSETTYPE="COUP" CPFPOS=AC(_CPFP) CPFMON="CONT"
ELSE
CPON=(_CPF) CPLA=(_CPL) CPSETTYPE="COUP" CPFMON="CONT"
ENDIF
ELSE
CPON=(_CPF) CPSETTYPE="COUP" CPFMON="CONT"
ENDIF
RET
_FEHL: SETAL(61017)
RET
