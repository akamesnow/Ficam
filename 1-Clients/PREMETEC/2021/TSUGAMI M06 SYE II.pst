        axec = atan2(y$,x$)        # Calcul de l'angle de rotation du mandrin axe C
        n$, axec, x_Bout_perce, *peck1$, depth$, garde, e$
    ]
    if spindle_no$ = 1,
    [
        garde = (refht$ - initht$)*  -1
        depth_d = depth$ *  -1
        axee = atan2(y$,-x$)        # Calcul de l'angle de rotation du mandrin axe C
        n$, axee, x_Bout_perce_d, *peck1$, depth_d, garde, e$
    ]
ppeckD       # Cycle de percage en fraisage en dessus
    garde = (refht$ - initht$)
    frein = 1
    pgerefrein
    if dwell$ = 0, n$, "G87", *x_Dessus, *prof, *garde, *peck1$, pfr_fraisage1, e$ #*frplunge$, e$
    else, n$, "G87", *x_Dessus, *prof, *garde, *peck1$, *tempo,  pfr_fraisage1, e$ #*frplunge$, e$
ppeckD_2       # Cycle de percage en fraisage en dessus
    pcanceldc$
    prot_calc
    prot_affich_T
    ppeckD
ppeckDev
ppeckDev_2
mchpbrk$   # Cycle de brise copeau en fraisage
    if dessus = 0, ppeckB
    if dessus = 1, ppeckD
    if dessus = 2, ppeckDev
mchpbrk_2$ # Canned Chip Break Cycle
    if dessus = 0, ppeckB_2
    if dessus = 1, ppeckD_2
    if dessus = 2, ppeckDev_2
mtap$      # Cycle de taraudage en fraisage
    fr_taraud = n_tap_thds$
    if dessus = 0, ptapB
    if dessus = 1, ptapD
     if dessus = 2, ptapDev
mtap_2$    # Taraudages suivants en fraisage
    if dessus = 0, ptapB_2
    if dessus = 1, ptapD_2
    if dessus = 2, ptapDev_2
ptapB     # Cycle de taraudage en fraisage en bout
    if spindle_no$ = 0,
    [
        !initht$, !depth$
        garde = refht$ - initht$
        garde1 = depth$
        garde2 = abs(ss$)
        garde3 = abs(ss$)
        gardeR = refht$
        axec = atan2(y$,x$), e$
        frein = 1
        pgerefrein
		n$, "M204 S0", e$
		n$, "M29", Vcc, e$
        if dwell$ = 0, n$, "G99 G84", *garde1,*fr_taraud, *gardeR, e$
        else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
        !x$, !y$, !z$
    ]
    if spindle_no$ = 1,
    [
        garde = (refht$ - initht$)*  -1
        axee = atan2(y$,-x$), e$
        frein = 1
        pgerefrein
        depth_d = depth$ *  -1
		n$, spdlon, "S0", e$
		n$, "M29", ss$, e$
        if dwell$ = 0, n$, " G99 G84", *garde1,*fr_taraud, *gardeR, e$
        else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
    ]
ptapB_2
    garde = refht$ - initht$
    if spindle_no$ = 0,
    [
        !initht$, !depth$
        garde = refht$ - initht$
        garde1 = depth$
        garde2 = abs(ss$)
        garde3 = abs(ss$)
        gardeR = refht$
        axec = atan2(y$,x$), e$
        frein = 3
        pgerefrein
        n$, *axec, e$
        frein = 1
        pgerefrein
        if dwell$ = 0, n$, "G99 G84", *garde1,*fr_taraud, *gardeR, e$
        else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
        !x$, !y$, !z$
    ]
    if spindle_no$ = 1,
    [
        garde = (refht$ - initht$)*  -1
        depth_d = depth$ *  -1
        axee = atan2(y$,-x$)        # Calcul de l'angle de rotation du mandrin axe C
        n$, "G84", *garde1, *gardeR, *fr_taraud, e$
    ]
ptapD     # Cycle de taraudage en fraisage en dessus
    !initht$, !depth$
    garde = refht$ - initht$
    garde1 = depth$ *2
    garde2 = Vccf
    garde3 = Vccf
    gardeR = refht$ - initht$
    frein = 1
    pgerefrein
    if dwell$ = 0, n$, "G99 G84", *garde1,*fr_taraud, *gardeR, e$
    else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
    !x$, !y$, !z$
ptapD_2       # Cycle de taraudage en fraisage en dessus
    pcanceldc$
    prot_calc
    prot_affich_T
    ptapD
ptapDev
	 if spindle_no$ = 0,
    [
        !initht$, !depth$
        garde = refht$ - initht$
        garde1 = depth$
		garde1X = depth$
        garde2 = abs(ss$)
        garde3 = abs(ss$)
        gardeR = refht$
        axec = atan2(y$,x$), e$
        frein = 1
        pgerefrein
		n$, "M203 S0", e$
		n$, "M29", Vcc, e$
        if dwell$ = 0, n$, "G99 G84", *garde1X ,*fr_taraud, *gardeR, e$
        else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
        !x$, !y$, !z$
    ]
    if spindle_no$ = 1,
    [
        garde = (refht$ - initht$)*  -1
        axee = atan2(y$,-x$), e$
        frein = 1
        pgerefrein
        depth_d = depth$ *  -1
		n$, spdlon, "S0", e$
		n$, "M29", ss$, e$
        if dwell$ = 0, n$, " G99 G84", *garde1,*fr_taraud, *gardeR, e$
        else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
    ]
ptapDev_2
	n$ "M79", e$
	n$, axec, e$
	n$ "M78", e$
	if dwell$ = 0, n$, " G99 G84", *garde1,*fr_taraud, *gardeR, e$
       else, n$, "G84", *garde1, *gardeR, *fr_taraud, e$
#--------------------------------------------------------------------
mbore1$        # Cycle d'alesage en fraisage
    if dessus = 0, pbore1B
    if dessus = 1, pbore1D
    if dessus = 2, pbore1Dev
mbore1_2$  # Cycle d'alesage en fraisage
    if dessus = 0, pbore1B_2
    if dessus = 1, pbore1D_2
    if dessus = 2, pbore1Dev_2
pbore1B       # Cycle d'alesage en fraisage en bout
    if spindle_no$ = 0,
    [
        garde = refht$ - initht$
        axec = atan2(y$,x$), e$
        frein = 1
