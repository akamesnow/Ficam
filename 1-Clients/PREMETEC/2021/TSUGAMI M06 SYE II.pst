    z$ = initht$
    if n_tap_thds$ <> 0, n$, "G0", "G80"
     else, n$, "G0", "G80"
    if dessus = 0,
    [
        if spindle_no$ = 0, *z_Bout, e$
        if spindle_no$ = 1, *z_Bout_d, e$
    ]
    if dessus = 1,
    [
        if spindle_no$ = 0, *z_Dessus, e$
        if spindle_no$ = 1, *z_Dessus, e$
    ]
    if dessus = 2,
    [
        if spindle_no$ = 0, *z_dev, e$
        if spindle_no$ = 1, *z_dev, e$
     ]


